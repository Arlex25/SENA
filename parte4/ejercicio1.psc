
Algoritmo ejercicio1
	Definir clave Como entero;
	Repetir
		Escribir "Escribir clave: "; Leer clave;
		Escribir "Usted ha elegido el número ", clave, " pero no es el número correcto. Inténtalo de nuevo"; 
	Hasta Que clave == 4; 
	Escribir "Ha escrito el número correcto. Hasta luego";	
FinAlgoritmo
