Algoritmo ejercicio5
	Dimension meses[12];
	meses[1] <- "enero"; meses[2] <- "febrero"; meses[3] <- "marzo";
	meses[4]
	Para var1<-1 Hasta 12 Con Paso 1 Hacer
		Dimension 
		Escribir "Mes: ", var1;
		Para var2 <- 1 Hasta 30 Con Paso 1 Hacer
			resultado <- var1*var2;
			Escribir Sin Saltar var1, "x", var2, " = ", resultado, ",  " ;
		FinPara
		
		Escribir " ";
	FinPara
	
FinAlgoritmo
